<!doctype html>
<html>
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <title>Sitename</title>
  {{= it.head() }}
</head>
<body>
  <header class="head">
    <h1 class="title">Sitename</h1>
  </header>
  <section class="wrap global-width">
    {{ if(it.hasPost()) { }}
      {{ while(it.hasPost()) { }}
        {{ it.thePost(); }}
          <article class="post">
            <header class="clearfix">
              <h2>
                <a href="{{= it.post.url.view }}">{{= it.post.title }}</a>
              </h2>
              <p class="timestamp">
                {{= it.post.created('d F, Y') }}
              </p>
            </header>
            <section class="entry">
              {{= it.post.content }}
            </section>
          </article>
      {{ } }}
    {{ } }}
  </section>
  <footer>
    <p>Something nice</p>
  </footer>
  {{= it.foot() }}
</body>
</html>
